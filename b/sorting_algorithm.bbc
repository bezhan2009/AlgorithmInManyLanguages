REM Процедура сортировки пузырьком
DEFPROCbubbleSort(arr())
  LOCAL n, i, j, temp
  n = FNsize(arr)
  FOR i = 0 TO n - 2
    FOR j = 0 TO n - 2 - i
      IF arr(j) > arr(j + 1) THEN
        temp = arr(j)
        arr(j) = arr(j + 1)
        arr(j + 1) = temp
      END IF
    NEXT j
  NEXT i
END PROC

REM Пример использования
DIM arr(4)
arr(0) = 5
arr(1) = 3
arr(2) = 8
arr(3) = 4
arr(4) = 2

CALL bubbleSort(arr)

FOR i = 0 TO 4
  PRINT arr(i)
NEXT i
