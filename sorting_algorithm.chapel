proc bubbleSort(arr: [7] int) {
    var swapped: bool;
    repeat {
        swapped = false;
        for i in 1..6 {
            if arr[i] > arr[i+1] {
                swap(arr, i, i+1);
                swapped = true;
            }
        }
    } until !swapped;
}

proc swap(arr: [7] int, i: int, j: int) {
    var temp = arr[i];
    arr[i] = arr[j];
    arr[j] = temp;
}

var arr: [7] int = [64, 34, 25, 12, 22, 11, 90];
bubbleSort(arr);
writeln("Sorted array: ", arr);
