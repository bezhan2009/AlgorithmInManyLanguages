def bubbleSort(arr as List<int>):
    n = arr.count
    for i in range(n - 1):
        for j in range(n - 1 - i):
            if arr[j] > arr[j + 1]:
                temp = arr[j]
                arr[j] = arr[j + 1]
                arr[j + 1] = temp

# Example usage
arr = List.of(5, 3, 8, 4, 2)
bubbleSort(arr)
print(arr)
