// Пример сортировки пузырьком на Fjölnir
function bubbleSort(arr) {
    n := length(arr);
    for i := 0 to n - 2 do {
        for j := 0 to n - 2 - i do {
            if arr[j] > arr[j + 1] then {
                temp := arr[j];
                arr[j] := arr[j + 1];
                arr[j + 1] := temp;
            }
        }
    }
    return arr;
}

// Пример использования
arr := [5, 3, 8, 4, 2];
bubbleSort(arr);
print(arr);
